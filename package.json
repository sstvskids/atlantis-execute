{
	"name": "atlantis-execute",
	"displayName": "Atlantis Execute Extension",
	"description": "Executes the code from your Visual Studio Code's current tab",
	"icon": "oxygen-u.png",
	"author": "kaisei-kto",
	"publisher": "sstvskids",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.60.0"
	},
	"repository": {
		"url": "https://github.com/sstvskids/atlantis-execute.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "o2u-execute.execute",
				"title": "O2U: Execute Script"
			},
			{
				"command": "o2u-execute.executeFile",
				"title": "O2U: Execute File"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorLangId == lua || resourceExtname == .lua || resourceExtname == .luau",
					"command": "o2u-execute.execute",
					"group": "navigation"
				}
			],
			"explorer/context": [
				{
					"when": "editorLangId == lua || resourceExtname == .lua || resourceExtname == .luau",
					"command": "o2u-execute.executeFile",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.60.0",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"ws": "^8.8.1"
	}
}
